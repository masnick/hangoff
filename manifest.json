{
  "name": "Hangoff, aka Google-",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Less Google+, more hangout chats.",
  "permissions": [
    "https://plus.google.com/*"
  ],
  "content_scripts" : [
    {
      "matches" : [ "https://plus.google.com/*" ],
      "js": ["hangoff.js"],
      "run_at" : "document_start"
    }
  ]
}